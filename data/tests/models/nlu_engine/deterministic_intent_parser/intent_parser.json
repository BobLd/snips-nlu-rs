  },
  "language_code": "en",
  "patterns": {
    "MakeCoffee": [
      "^\\s*make\\s*me\\s*(?P<group_2>%SNIPSNUMBER%)\\s*cups\\s*of\\s*coffee\\s*$",
      "^\\s*Brew\\s*me\\s*(?P<group_1>%SNIPSNUMBER%)\\s*coffee\\s*cups\\s*$",
